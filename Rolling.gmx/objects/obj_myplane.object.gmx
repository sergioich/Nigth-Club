    if( can_shoot == 1){
        instance_create(x,y-16,obj_back_bullet);
        can_shoot = 0;
        if(w==1){
        alarm[0] = 15;
        }else{ alarm[1] = 5;}
    if(pp&gt;=1){
    instance_create(x-16,y-16,obj_back_bullet);
    instance_create(x+16,y-16,obj_back_bullet);
    if(w==1){
        alarm[0] = 15;
        }else{ alarm[1] = 5;}
    }
    }
    
}

// Left control
