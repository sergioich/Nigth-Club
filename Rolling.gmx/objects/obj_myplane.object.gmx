    if(y &lt; view_yview[0] + 480 - 120){
        y += 2;
    }
}
// double shoot
if(a == true){
if(double == 2){
double -=1; 
w += 1; 
    }
    else{
    double +=1;
    w -= 1;
    }
}

    
        
    
    


